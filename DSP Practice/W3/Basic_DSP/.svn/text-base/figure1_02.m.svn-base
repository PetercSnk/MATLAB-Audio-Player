[u1,FS,NBITS]=wavread('ton2.wav');
y=interp(u1(1:30),4);

subplot(2,4,1);
plot((0:96)*5,y(1:97));
title('x(t)');
axis([0 500 -0.05 0.05]);
xlabel('t in \musec \rightarrow');
subplot(2,4,2);
stem((0:24),u1(1:25),'.');axis([0 25 -0.05 0.05]);
xlabel('n \rightarrow');
title('x(n)');
subplot(2,4,3);
stem((0:24),0.5*u1(1:25),'.');axis([0 25 -0.05 0.05]);
xlabel('n \rightarrow');
title('y(n)');
subplot(2,4,4);
plot((0:96)*5,0.5*y(1:97));axis([0 500 -0.05 0.05]);
xlabel('t in \musec \rightarrow');
title('y(t)');